<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletVoto</servlet-name>
  	<display-name>ServletVoto</display-name>
  	<description></description>
  	<servlet-class>src.com.model.servlet.ServletVoto</servlet-class>
  	<init-param>
  	  <param-name>etapa</param-name>
  	  <param-value>vereador</param-value>
  	</init-param>
  </servlet>
  <servlet>
  	<servlet-name>API</servlet-name>
  	<display-name>API</display-name>
  	<description></description>
  	<servlet-class>src.com.model.servlet.API</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletVoto</servlet-name>
  	<url-pattern>/ServletVoto</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>API</servlet-name>
  	<url-pattern>/API</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>telavoto.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
